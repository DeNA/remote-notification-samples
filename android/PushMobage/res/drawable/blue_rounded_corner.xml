<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="#295169"
                android:endColor="#2F7BBF"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#2f6699" />
            <corners
                android:radius="7dp" />
        </shape>
    </item>
    <item android:state_enabled="false" >
        <shape>
            <gradient
                android:startColor="#9D9D9D"
                android:endColor="#7F7F7F"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#2f6699" />
            <corners
                android:radius="7dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#65B4E3"
                android:endColor="#2F7BBF"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#000000" />
            <corners
                android:radius="7dp" />
        </shape>
    </item>
</selector>